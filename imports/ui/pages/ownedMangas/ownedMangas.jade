template(name="ownedMangas")
	if Template.subscriptionsReady
		if tomesInMangatek
			.panel.panel-default
				.panel-heading
					h2.panel-title {{_ "allTomesOwned"}}
				.panel-body
					+buttonAddMangaForUser
					.grid
						each mangasList
							+mangas
				.panel-footer
					+buttonAddMangaForUser
		else
			.panel.panel-default
				.panel-heading
					h2.panel-title {{_ "noTomesInMangatek"}}
				.panel-body
					+buttonAddMangaForUser
	else
		+spinner